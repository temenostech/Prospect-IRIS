domain common {
    use common.T24Commands.*
    use common.HTTPEvents.*
        
    rim NextState {        
        basepath: "/{companyid}"
        resource nextState {
        type: item
               entity: NextState
               commands [ GET: T24NextState ]
               relations [ "http://temenostech.temenos.com/rels/next" ]
               path: "/nextState"
               GET --> locator locatorNextState("{NextState}" )
        }        
    }    
}
